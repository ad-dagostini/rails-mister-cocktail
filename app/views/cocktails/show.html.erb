<div class="banner">
  <div class="d-flex justify-content-center mb-3 mt-3 p-4">
    <%= image_tag "data:image/jpeg;base64,", width: 210, height: 220, crop: :fill %>
    <div class="header mb-3 mt-3 p-4"><h1><strong>Cocktails<strong></h1></div>
  </div>
</div>
<div class="container p-5 mt-3">
  <h2><%= @cocktail.name %></h2>
  <h4><strong>Recipe<strong></h4>
    <% @cocktail.doses.each do |dose| %>
    <div class="d-flex align-items-center">
      <h4><%= dose.description%> of <%= dose.ingredient.name %> | </h4>
      <div class="ml-2">
        <h6><%= link_to "DELETE", dose_path(dose), method: :delete, data: { confirm: 'You sure?'} %><h6>
      </div>
    </div>
  <% end %>
</div>

<div>
</div>

<div class="col-md-6 p-5 mt-3">
  <%= simple_form_for [@cocktail, @dose] do |f| %>
    <%= f.association :ingredient %>
    <%= f.input :description %>
    <%= f.submit %>
  <% end %>
</div>
